{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Ambience",
  "description": "Replace your new tab with limitless AI-generated wallpapers that refresh every hour.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ]
}
